<%= simple_form_for(@artwork) do |f| %>
  <%= f.input :name, label: 'Name of the artwork' %>
  <%= f.input :category %>
  <%= f.input :description , label: 'A brief description of the artwork' %>
  <%= f.input :rate, label: 'Rental price per month' %>
  <%= f.input :photo, label: 'Photo URL' %>
  <%= f.button :submit %>
<% end %>